
#include<bits/stdc++.h>
using namespace std;

const int mx = 1e3+123;
char adjMat[mx][mx];

/// adjMat[i][j] means there is a edge between i and j node.

int main()
{
    optimize();

    int n, m;
    cin >> n >> m;

    for ( int i = 1; i <= n; i++ ) {
        for ( int j = 1; j <= m; j++ ) cin >> adjMat[i][j];
    }


    return 0;
}
